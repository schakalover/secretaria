<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    @Styles.Render("~/Content/css")
    @RenderSection("styles", required: false)
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
</head>
<body>
    <ul id="menu">
        <li>@Html.ActionLink("Inicio", "Index", "Home")</li>
        <li>@Html.ActionLink("Menu", "Contact", "Home")</li>
        <li>@Html.ActionLink("Catalagos", "Index", "Catalagos")</li>
        <li>@Html.ActionLink("Inventario", "Index", "Inventario")</li>
        <!-- <li>@Html.ActionLink("Raciones", "About", "Home")</li>-->
        <li>@Html.ActionLink("Peticiones Especiales", "Peticiones", "Home")</li>
        <li>@Html.ActionLink("Reportes", "Reportes", "Home")</li>
        <li>@Html.ActionLink("Nuevo Racionamiento", "Racionamiento", "Home")</li>
    </ul>

    <section id="main">

        <img alt="" src="../Content/imagenes/ssypc.PNG" />
        <h1>CATALAGOS</h1>

        <ul class="nav nav-tabs">
            <li class="active"><a href="@Url.Action("Index")">Inicio</a></li>
            <li class="dropdown">
                <a class="dropdown-toggle" data-toggle="dropdown" href="#">PRODUCTO<span class="caret"></span></a>
                <ul class="dropdown-menu">
                    <li><a href="@Url.Action("AltaProducto")">Alta Producto</a></li>
                    <li><a href="@Url.Action("BajaProducto")">Baja Producto</a></li>
                    <li><a href="@Url.Action("ConsultaProducto")">Consulta Producto</a></li>
                </ul>
            </li>
            <li class="dropdown">
                <a class="dropdown-toggle" data-toggle="dropdown" href="#">MARCA<span class="caret"></span></a>
                <ul class="dropdown-menu">
                    <li><a href="#">Alta Marca</a></li>
                </ul>
            </li>
            <li class="dropdown">
                <a class="dropdown-toggle" data-toggle="dropdown" href="#">MODELO<span class="caret"></span></a>
                <ul class="dropdown-menu">
                    <li><a href="#">Alta Modelo</a></li>
                </ul>
            </li>
            <li class="dropdown">
                <a class="dropdown-toggle" data-toggle="dropdown" href="#">UNIDADES DE MEDIDA<span class="caret"></span></a>
                <ul class="dropdown-menu">
                    <li><a href="#">Alta Unidades</a></li>
                </ul>
            </li>
        </ul>

        @RenderBody()
        <center><p>Secretaria de Seguridad y Proteccion ciudadana.</p></center>
    </section>

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
